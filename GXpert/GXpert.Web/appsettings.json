{
  "Data": {
    "Default": {
      "ConnectionString": "Server=(local);Database=GXpert_Default_v11;Integrated Security=true;Encrypt=false;TrustServerCertificate=true",
      "ProviderName": "System.Data.SqlClient"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "BackgroundJobs": {
    "Enabled": true
  },
  "ClamAV": {
    "Enabled": false
  },
  "EnvironmentSettings": {
    "SiteExternalUrl": null
  },
  "SmtpSettings": {
    "Host": null,
    "Port": 25,
    "UseSsl": false,
    "From": null,
    "PickupPath": "App_Data/Mail"
  },
  "EmailQueueJob": {
    "Enabled": true,
    "AutoUse": true,
    "Interval": 30,
    "BatchSize": 10,
    "LockDuration": 300,
    "RetryLimit": 5
  },
  "CssBundling": {
    "Enabled": false,
    "Minimize": true,
    "UseMinCss": true
  },
  "OpenIdSettings": {
    "EnableServer": false,
    "EnableClient": false,
    "Scopes": [
      { "Name": "profile" },
      { "Name": "email" },
      { "Name": "worklog", "PermissionFilters": [ "Regex:^WorkLog:.*" ] }
    ],
    "Applications": [
    ]
  },
  "ScriptBundling": {
    "Enabled": false,
    "Minimize": true,
    "UseMinJS": true
  },
  "UploadSettings": {
    "Path": "~/App_Data/upload/"
  },
  "UseForwardedHeaders": true
}